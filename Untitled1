import java.util.Scanner;

class ex43 {

public static void main(String[] args){
Scanner stdIn = new Scanner(System.in);

System.out.println("小さい数から大きい数までの数字を表示します。");

System.out.println("整数Ａ:"); int a = stdIn.nextInt(); 
System.out.println("整数Ｂ:"); int b = stdIn.nextInt();
System.out.println();
if (a > b){
int t = a;;
a = b;
b = t;
}

do { (
System.out.println(a++);

} while (a != b);
}
}